            <lido:inscriptionsWrap>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="1">
                  <lido:inscriptionTranscription>Will fecit 1742 [seitenverkehrt]</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>oben Mitte oberhalb der Darstellung signiert und datiert</lido:descriptiveNoteValue>
                  </lido:inscriptionDescription>
               </lido:inscriptions>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="10">
                  <lido:inscriptionTranscription>Pesne pin.t ["t" hochgestellt] à Berlin</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>unten links innerhalb der Darstellung auf der Deckplatte der Konsole bezeichnet</lido:descriptiveNoteValue>
                  </lido:inscriptionDescription>
               </lido:inscriptions>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="15">
                  <lido:inscriptionTranscription>J. G. Will S. à Paris</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>unten rechts innerhalb der Darstellung auf der Deckplatte der Konsole signiert</lido:descriptiveNoteValue>
                  </lido:inscriptionDescription>
               </lido:inscriptions>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="20">
                  <lido:inscriptionTranscription>s’il fut. par sa naissance, au Trône destiné. Les droits de ses vertus sont ils moins légitimes? Héros dans ses actions. Heros dans ses Maximes. Il est Roy philosophe et Soldat couronné.</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>unten Mitte auf dem Postament bezeichnet</lido:descriptiveNoteValue>
                  </lido:inscriptionDescription>
               </lido:inscriptions>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="25">
                  <lido:inscriptionTranscription>Par M.r ["r" hochgestellt] le Chevalier de Neufville.</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>unten Mitte innerhalb der Darstellung bezeichnet</lido:descriptiveNoteValue>
                  </lido:inscriptionDescription>
               </lido:inscriptions>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="30">
                  <lido:inscriptionTranscription>a Paris chez Petit rue S. Jacques a la Couronne d’épines pres les Mathurins.</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>unten am Plattenrand bezeichnet</lido:descriptiveNoteValue>
                  </lido:inscriptionDescription>
               </lido:inscriptions>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="5">
                  <lido:inscriptionTranscription>CHARLES FREDERIC, ROY DE PRUSSE ELECTEUR DE BRANDENBOURG Né à Berlin le 24 Janvier 1712.</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>umlaufend um das Porträtmedaillon betitelt</lido:descriptiveNoteValue>
                  </lido:inscriptionDescription>
               </lido:inscriptions>
