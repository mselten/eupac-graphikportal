               <lido:inscriptions lido:type="Inschrift" lido:sortorder="1">
                  <lido:inscriptionTranscription>Ludolph Bakhuizen Pinx,,t ["t" hochgestellt]</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>unten links unterhalb der Darstellung bezeichnet</lido:descriptiveNoteValue>
                  </lido:inscriptionDescription>
               </lido:inscriptions>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="15">
                  <lido:inscriptionTranscription>From the Original Picture painted by Ludolph Bakhuizen, In the Collection of the Right Honourable the Earl of Bute: TO whom this PLATE is , ~ ~ with the greatest Respect, most humbly Dedicated, by his LORDSHIP'S, most Obliged, and most Obedient Servant, J. BOYDELL.</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>unterhalb der Darstellung bezeichnet</lido:descriptiveNoteValue>
                  </lido:inscriptionDescription>
               </lido:inscriptions>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="20">
                  <lido:inscriptionTranscription>N,,°.22.</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>unten links am Plattenrand nummeriert</lido:descriptiveNoteValue>
                  </lido:inscriptionDescription>
               </lido:inscriptions>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="25">
                  <lido:inscriptionTranscription>Size of the Picture 2F ["F" über der "2"]: oi ["i" über dem "o"] by 2F ["F" über der "2"]: 9i ["i" über der "9"] in Length</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>unten links am Plattenrand bezeichnet</lido:descriptiveNoteValue>
                  </lido:inscriptionDescription>
               </lido:inscriptions>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="30">
                  <lido:inscriptionTranscription>Published according to Act of Parliament, by J. BOYDELL Engraver, in Cheapside LONDON: February 1st. 1766.</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>unten links und rechts neben dem Wappen datiert und bezeichnet</lido:descriptiveNoteValue>
                  </lido:inscriptionDescription>
               </lido:inscriptions>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="35">
                  <lido:inscriptionTranscription>HONI · SOIT · QUI · MAL · Y · PENSE</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>im ovalen Rahmen des Wappens bezeichnet</lido:descriptiveNoteValue>
                  </lido:inscriptionDescription>
               </lido:inscriptions>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="40">
                  <lido:inscriptionTranscription>AVITO · VIRET · HONORE</lido:inscriptionTranscription>
