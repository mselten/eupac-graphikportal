               <lido:inscriptions lido:type="Inschrift" lido:sortorder="1">
                  <lido:inscriptionTranscription>CHARLES FREDERIC</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>oben Mitte umlaufend um das Porträtmedaillon betitelt</lido:descriptiveNoteValue>
                  </lido:inscriptionDescription>
               </lido:inscriptions>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="10">
                  <lido:inscriptionTranscription>Peint par J. F. Guillibaud</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>unten links auf der Deckplatte der Konsole bezeichnet</lido:descriptiveNoteValue>
                  </lido:inscriptionDescription>
               </lido:inscriptions>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="15">
                  <lido:inscriptionTranscription>Gravé par Will.</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>unten rechts auf der Deckplatte der Konsole signiert</lido:descriptiveNoteValue>
                  </lido:inscriptionDescription>
               </lido:inscriptions>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="20">
                  <lido:inscriptionTranscription>Du Prince que tu vois le rang et la naissance Sont les Titres les moins flateurs; Il ne se sert de sa Puissance Que pour soumettre tous les Cœurs.</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>unten Mitte auf dem Postament bezeichnet</lido:descriptiveNoteValue>
                  </lido:inscriptionDescription>
               </lido:inscriptions>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="25">
                  <lido:inscriptionTranscription>A Lausanne et à Geneve, chez MARC MICHEL BOUSQUET et Comp.e ["e" hochgestellt] 1745.</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>unten Mitte unterhalb der Darstellung am Blattrand bezeichnet</lido:descriptiveNoteValue>
                  </lido:inscriptionDescription>
               </lido:inscriptions>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="5">
                  <lido:inscriptionTranscription>Margrave de Bade et Hachberg &amp;c. &amp;c. &amp;c. agé de XVII. ans.</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>Mitte umlaufend um das Porträtmedaillon bezeichnet</lido:descriptiveNoteValue>
