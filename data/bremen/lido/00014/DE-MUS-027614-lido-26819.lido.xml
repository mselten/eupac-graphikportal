            <lido:inscriptionsWrap>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="1">
                  <lido:inscriptionTranscription>N.° IV.</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>oben links oberhalb der Darstellung nummeriert</lido:descriptiveNoteValue>
                  </lido:inscriptionDescription>
               </lido:inscriptions>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="10">
                  <lido:inscriptionTranscription>A Monsieur Pierre Joseph Victor de Besenval Baron de Bronnstatt, Grand Croix de l'Ordre Royal et Militaire de S.t ["t" hochgestellt] Louis, Lieutenant général des Armées du Roy, Inspecteur général des Regimens Suisses et Grisons Gouverneur de la Ville d'Haguenau en Alsaçe Lieutenant Colonel du Regiment des Gardes Suißes</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>unterhalb der Darstellung mit Widmungstext bezeichnet</lido:descriptiveNoteValue>
                  </lido:inscriptionDescription>
               </lido:inscriptions>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="15">
                  <lido:inscriptionTranscription>Par son très humble et très Obeissant Serviteur Choffard.</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>unten rechts innerhalb der Darstellung signiert</lido:descriptiveNoteValue>
                  </lido:inscriptionDescription>
               </lido:inscriptions>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="20">
                  <lido:inscriptionTranscription>Peint a la Gouasse par P. A. Baudouin Peintre du Roy 1764.</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>unten links unterhalb der Darstellung bezeichnet</lido:descriptiveNoteValue>
                  </lido:inscriptionDescription>
               </lido:inscriptions>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="25">
                  <lido:inscriptionTranscription>du Cabinet de M.r ["r" hochgestellt] Besenval.</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>unten Mitte unterhalb der Darstellung bezeichnet</lido:descriptiveNoteValue>
                  </lido:inscriptionDescription>
               </lido:inscriptions>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="30">
                  <lido:inscriptionTranscription>Gravé par P.P. Choffard 1767 AParis ["AP" ligiert] 1.re ["re" hochgestellt] Cour des Quinze vingts.</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>unten rechts unterhalb der Darstellung signiert und datiert</lido:descriptiveNoteValue>
                  </lido:inscriptionDescription>
               </lido:inscriptions>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="35">
                  <lido:inscriptionTranscription>23684 [Inventarnummer]</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>unten rechts am Blattrand von fremder Hand mit Bleistift nummeriert</lido:descriptiveNoteValue>
                  </lido:inscriptionDescription>
               </lido:inscriptions>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="5">
                  <lido:inscriptionTranscription>B</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>oben rechts oberhalb der Darstellung bezeichnet</lido:descriptiveNoteValue>
