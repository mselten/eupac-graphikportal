            <lido:inscriptionsWrap>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="1">
                  <lido:inscriptionTranscription>23.te ["te" hochgestellt] Pl..</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>oben links oberhalb der Darstellung nummeriert</lido:descriptiveNoteValue>
                  </lido:inscriptionDescription>
               </lido:inscriptions>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="10">
                  <lido:inscriptionTranscription>Graveur du Roi.</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>unten rechts am Darstellungsrand bezeichnet</lido:descriptiveNoteValue>
                  </lido:inscriptionDescription>
               </lido:inscriptions>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="15">
                  <lido:inscriptionTranscription>SAPEUR DES GARDES SUISSES</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>unten Mitte unterhalb der Darstellung das zentrale Wappen flankierend betitelt</lido:descriptiveNoteValue>
                  </lido:inscriptionDescription>
               </lido:inscriptions>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="20">
                  <lido:inscriptionTranscription>A Monsieur le Baron de Soursanvault Chevau-Leger de la Garde du Roi.</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>unterhalb der Darstellung mit Widmungstext bezeichnet</lido:descriptiveNoteValue>
                  </lido:inscriptionDescription>
               </lido:inscriptions>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="25">
                  <lido:inscriptionTranscription>A Paris chez l’Auteur, Quai des Augustins.</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>unten links am Plattenrand bezeichnet</lido:descriptiveNoteValue>
                  </lido:inscriptionDescription>
               </lido:inscriptions>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="30">
                  <lido:inscriptionTranscription>Par son Ami et très humble Serviteur, Wille</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>unten rechts am Plattenrand signiert</lido:descriptiveNoteValue>
                  </lido:inscriptionDescription>
               </lido:inscriptions>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="35">
                  <lido:inscriptionTranscription>STAT VIRTUS NIXA FIDE</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>unten Mitte auf dem Banner des Wappens bezeichnet</lido:descriptiveNoteValue>
                  </lido:inscriptionDescription>
               </lido:inscriptions>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="5">
                  <lido:inscriptionTranscription>Dessiné et gravé par Wille</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>unten links unterhalb der Darstellung signiert</lido:descriptiveNoteValue>
                  </lido:inscriptionDescription>
