            <lido:inscriptionsWrap>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="1">
                  <lido:inscriptionTranscription>BENJAMIN FRANKLIN.</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>unterhalb des Portraits betitelt</lido:descriptiveNoteValue>
                  </lido:inscriptionDescription>
               </lido:inscriptions>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="10">
                  <lido:inscriptionTranscription>Dessiné par C. N. Cochin Chevalier de l'Ordre du Roi, en 1777 et Gravé par Aug. de S.t ["t" hochgestellt] Aubin Graveur de la Bibliotheque du Roi.</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>unterhalb der Darstellung signiert und datiert</lido:descriptiveNoteValue>
                  </lido:inscriptionDescription>
               </lido:inscriptions>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="15">
                  <lido:inscriptionTranscription>Se vend à Paris chés C. N. Cochin aux Galleries du Louvre; et chés Aug. de S.t ["t" hochgestellt] Aubin, rue des Mathurins.</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>unten Mitte unterhalb der Darstellung bezeichnet</lido:descriptiveNoteValue>
                  </lido:inscriptionDescription>
               </lido:inscriptions>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="5">
                  <lido:inscriptionTranscription>Né à Boston, dans la nouvelle Angleterre le 17. Janvier 1706.</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>unten innerhalb der Darstellung bezeichnet</lido:descriptiveNoteValue>
                  </lido:inscriptionDescription>
               </lido:inscriptions>
