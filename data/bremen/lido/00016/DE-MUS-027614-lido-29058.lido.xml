            <lido:inscriptionsWrap>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="1">
                  <lido:inscriptionTranscription>REPAS DES MOISSONNEURS</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>unten Mitte das zentrale Wappen flankierend betitelt</lido:descriptiveNoteValue>
                  </lido:inscriptionDescription>
               </lido:inscriptions>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="10">
                  <lido:inscriptionTranscription>Gravé par f. Janinet en 1774. d’Après le Dessin Original grandeur fait par Pierre Allexandre Wille Peintre du Roi</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>unten links unterhalb der Darstellung signiert, datiert und bezeichnet</lido:descriptiveNoteValue>
                  </lido:inscriptionDescription>
               </lido:inscriptions>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="15">
                  <lido:inscriptionTranscription>à Paris chez Le Pere et Avaulez M.ds ["ds" hochgestellt] d’Estampes rue S.t ["t" hochgestellt] Jacques et chez l’Auteur rue de l’Hirondelle vis-à-vis l’Ecole gratuite</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>unten Mitte am Blattrand bezeichnet</lido:descriptiveNoteValue>
                  </lido:inscriptionDescription>
               </lido:inscriptions>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="20">
                  <lido:inscriptionTranscription>Imprimé par J. [C. Serg]ent fils.</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>unten Mitte links am Blattrand bezeichnet</lido:descriptiveNoteValue>
                  </lido:inscriptionDescription>
               </lido:inscriptions>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="25">
                  <lido:inscriptionTranscription>Par son très humble et très obéissant Serviteur F. Janinet</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>unten rechts am Blattrand bezeichnet</lido:descriptiveNoteValue>
                  </lido:inscriptionDescription>
               </lido:inscriptions>
               <lido:inscriptions lido:type="Inschrift" lido:sortorder="5">
                  <lido:inscriptionTranscription>Dédié à Monsieur le Comte de Baudouin Brigadier des Armées du Roi, Capitaine aux Gardes Françoises.</lido:inscriptionTranscription>
                  <lido:inscriptionDescription>
                     <lido:descriptiveNoteValue>unterhalb der Darstellung mit Widmungstext bezeichnet</lido:descriptiveNoteValue>
                  </lido:inscriptionDescription>
               </lido:inscriptions>
